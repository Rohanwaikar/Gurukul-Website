{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}
